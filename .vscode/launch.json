{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: main_uni.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/main_uni.py",
            "args": [
                "--experiment_description",
                "WISDM-RAINCOAT-uni",
                "--da_method",
                "RAINCOAT",
                "--dataset",
                "WISDM",
                "--backbone",
